{
  "name": "tizen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ci": "vite",
    "dev:3000": "vite --port 3000",
    "dev:mem": "NODE_OPTIONS=--max-old-space-size=384 vite",
    "dev:mem:256": "NODE_OPTIONS=--max-old-space-size=256 vite",
    "dev:ci:mem": "NODE_OPTIONS=--max-old-space-size=384 vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:mem": "NODE_OPTIONS=--max-old-space-size=384 vite preview",
    "healthz": "node -e \"(async()=>{try{const http=require('http');const opts={hostname:'127.0.0.1',port:process.env.PORT||3000,path:'/healthz',timeout:1500};const req=http.request(opts,res=>{if(res.statusCode===200){process.stdout.write('OK\\n');process.exit(0)}else{process.stderr.write('bad status '+res.statusCode+'\\n');process.exit(2)}});req.on('error',()=>{process.stderr.write('not ready\\n');process.exit(3)});req.end()}catch(e){process.stderr.write((e&&e.message)||String(e));process.exit(4)}})()\"",
    "build:tizen": "vite build",
    "package:tizen": "node ./scripts/package-tizen.js",
    "build-and-package:tizen": "npm run build:tizen && npm run package:tizen"
  },
  "engines": {
    "node": ">=18.0.0 <20.0.0",
    "npm": ">=9"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^4.5.3"
  }
}
