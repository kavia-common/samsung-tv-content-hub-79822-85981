{
  "name": "tizen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "dev:3000": "PORT=3000 vite --host 0.0.0.0 --port 3000",
    "dev:strict": "PORT=3000 vite --host 0.0.0.0 --port 3000",
    "dev:mem": "NODE_OPTIONS=--max-old-space-size=2048 PORT=3000 vite --host 0.0.0.0 --port 3000",
    "dev:port": "PORT=3000 vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "lint": "eslint .",
